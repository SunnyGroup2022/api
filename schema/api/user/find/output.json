{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "description": "get user/:id User.find output data",
  "properties": {
    "id": {
      "type": "integer",
      "description": "User ID"
    },
    "name": {
      "type": "string",
      "description": "User name"
    },
    "email": {
      "type": "string",
      "description": "User email"
    },
    "email_verified": {
      "type": "integer",
      "description": "Email verification status（0: Unverified, 1: Verified）"
    },
    "created_on": {
      "type": "object",
      "description": "User registration time"
    },
    "last_login": {
      "type": "object",
      "description": "User last login time"
    },
    "last_online": {
      "type": "object",
      "description": "User last online time"
    },
    "logins": {
      "type": "integer",
      "description": "The number of times the user logged in"
    }
  },
  "additionalProperties": false
}