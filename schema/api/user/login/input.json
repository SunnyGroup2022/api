{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "description": "POST user/login User.login input data",
  "properties": {
    "email": {
      "type": "string",
      "description": "User email",
      "pattern": "^(?=.*[@]).{3,200}$"
    },
    "password": {
      "type": "string",
      "description": "User password"
    }
  },
  "additionalProperties": false,
  "required": [
    "email",
    "password"
  ]
}