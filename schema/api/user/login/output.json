{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "description": "POST user/login User.login output data",
  "properties": {
    "status": {
      "type": "integer",
      "description": "登入狀態（1: 登入成功）"
    },
    "token": {
      "type": "string",
      "description": "登入成功後核發的Token"
    },
    "expireTime": {
      "type": "object",
      "description": "Token 的有效時間"
    },
    "userId": {
      "type": "integer",
      "description": "會員編號"
    },
    "name": {
      "type": ["string", "null"],
      "description": "會員姓名"
    }
  },
  "additionalProperties": false
}