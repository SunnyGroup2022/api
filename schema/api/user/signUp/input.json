{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "description": "POST user/signUp User.signUp input data",
  "properties": {
    "email": {
      "type": "string",
      "description": "會員輸入帳號",
      "pattern": "^(?=.*[@]).{3,200}$"
    },
    "password": {
      "type": "string",
      "description": "會員輸入密碼",
      "pattern": "^(?=.*\\d)(?=.*[a-z])(?=.*[A-Z])(?=.*\\W).{8,20}$"
    },
    "password2": {
      "type": "string",
      "description": "會員輸入密碼確認",
      "pattern": "^(?=.*\\d)(?=.*[a-z])(?=.*[A-Z])(?=.*\\W).{8,20}$"
    }
  },
  "additionalProperties": false,
  "required": [
    "email",
    "password",
    "password2"
  ]
}
